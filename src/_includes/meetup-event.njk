<div class="my-4">
    {%- if meetup.link -%}
        <a href="{{ meetup.link }}"><strong>{{ meetup.name }}</strong></a>
    {%- else -%}
        <strong>{{ meetup.name }}</strong>
    {%- endif -%}
    {%- if meetup.venue -%}
        &nbsp;-&nbsp;
        {%- if meetup.venue.lat -%}
            <a href="https://www.google.com/maps/search/?api=1&query={{ meetup.venue.lat }},{{ meetup.venue.lon }}">{{ meetup.venue.name }}</a>
        {%- else -%}
            {{ meetup.venue.name }}
        {%- endif -%}
    {%- endif -%}
    <br>
    {{ meetup.time | timeToDate }}
</div>
    